<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
    <%= f.label :song_genre_id %>
  <%= f.collection_select :genre, Genre.all, :id, :name %>

  <%= f.submit %>
<% end %>
